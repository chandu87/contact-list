<%include ./partials/header%>  
<div class="container" style="margin-top:50px">
<h1>Contacts Page</h1>
<a href="/" class="btn btn-info">Home Page</a>
<a href="/contacts/add" class="btn btn-info">Add Contacts</a>
<a href="/contacts/edit" class="btn btn-info">Edit Contacts</a>
<hr>
<%contacts.forEach((contact)=>{%>
    <div class="card col-md-4">
        <div class="row">
        <div class="col-md-6">
                <label class="col-md-12" >Name : <%=contact.name%></label>
                <label class="col-md-12">Age : <%=contact.age%></label>
                <label class="col-md-12">ID : <%=contact.id%></label>
                <br>
            </div>
        </div>
    </div>
<%});%>

</div>
<% include ./partials/footer %>

